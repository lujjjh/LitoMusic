{
  "name": "lito",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently -k \"npm:start:react\" \"npm:start:windows\"",
    "start:react": "vite",
    "start:windows": "cd windows && cargo build && wait-on tcp:3000 && target\\debug\\lito.exe",
    "build": "npm run build:react && npm run build:windows",
    "build:react": "tsc && vite build",
    "build:windows": "cd windows && cargo build --target x86_64-pc-windows-msvc --release"
  },
  "dependencies": {
    "ahooks": "^2.10.9",
    "i18next": "^20.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-i18next": "^11.12.0",
    "react-router-dom": "^5.3.0",
    "styled-components": "^5.3.1"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "devDependencies": {
    "@prettier/plugin-xml": "^1.0.2",
    "@types/apple-music-api": "^0.4.1",
    "@types/musickit-js": "^1.0.1",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/react-router": "^5.1.16",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "^5.1.14",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "i18next-browser-languagedetector": "^6.1.2",
    "prettier": "^2.4.1",
    "prettier-plugin-organize-imports": "^2.3.3",
    "prettier-plugin-style-order": "^0.2.2",
    "typescript": "^4.4.2",
    "vite": "^2.5.10",
    "vite-plugin-compression": "^0.3.5",
    "vite-react-jsx": "^1.1.2",
    "wait-on": "^6.0.0"
  }
}
